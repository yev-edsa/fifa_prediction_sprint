   "source": [
    "def bookie(certainty, home, away):\n",
    "    if certainty > home:\n",
    "        return 'home_win'\n",
    "    if certainty < away:\n",
    "        return 'away_win'\n",
    "    else:\n",
    "        return 'draw'    "
   ]
  },
  {
   "cell_type": "code",
